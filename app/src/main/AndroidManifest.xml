<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xyz.nlaz.albus">

    <user-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReviewActivity"/>
        <activity android:name=".CreateMomentActivity" />
        <activity android:name=".AccountActivity"/>
        <activity android:name=".SettingsOption" />
        <activity android:name=".MomentsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ReviewActivity" />
        </activity>

        <receiver android:name=".Notification_reciever" />


    </application>

</manifest>